{
  "manifest_version": 2,

  "name": "Chrome ESLint",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "source/eslint-logo.png",
    "default_popup": "source/popup.html"
  },

  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/"
  ]
}
